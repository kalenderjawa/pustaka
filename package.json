{
  "name": "@kalenderjawa/pustaka",
  "version": "1.0.0-rc.4",
  "description": "Pustaka JavaScript untuk perhitungan, pengkonversian dan penanggalan Kalender Jawa Abadi",
  "type": "module",
  "main": "lib/kalenderjawa.min.cjs",
  "module": "lib/kalenderjawa.min.js",
  "browser": "lib/kalenderjawa.browser.min.js",
  "exports": {
    ".": {
      "require": "./lib/kalenderjawa.min.cjs",
      "import": "./lib/kalenderjawa.min.js",
      "browser": "./lib/kalenderjawa.browser.min.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "build": "bunx rollup --config",
    "doc": "bunx jsdoc -c jsdoc.conf.json -r src -d docs",
    "test": "bunx cross-env JEST_PUPPETEER_CONFIG=jest-puppeteer.config.cjs bunx jest --no-cache --coverage --testPathIgnorePatterns=\"_data.*\"",
    "test-watch": "bun run test -- --watch",
    "serve": "bun scripts/res.js && bun test_server.js",
    "test-no-esm": "docker build ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalenderjawa/pustaka.git"
  },
  "keywords": [
    "kalender",
    "jawa",
    "lambang",
    "windu",
    "pawukon",
    "sengkala",
    "kalender jawa",
    "pasaran",
    "candra sengkala",
    "penanggalan",
    "dinten",
    "dino",
    "tahun jawa",
    "penanggalan jawa",
    "kalender jawa abadi"
  ],
  "author": "Equan Pr.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kalenderjawa/pustaka/issues"
  },
  "homepage": "https://github.com/kalenderjawa/pustaka#readme",
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-flow": "^7.27.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.4.1",
    "core-js": "^3.44.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-flowtype": "^8.0.3",
    "express": "^4.21.2",
    "flow-bin": "^0.170.0",
    "fs-extra": "^10.1.0",
    "jest": "^27.5.1",
    "jest-puppeteer": "^6.2.0",
    "puppeteer": "^13.7.0",
    "regenerator-runtime": "^0.13.11",
    "rollup": "^2.79.2",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "mjs"
    ],
    "transform": {
      "^.+\\.m?js?$": "babel-jest"
    },
    "preset": "jest-puppeteer",
    "globals": {
      "PATH": "http://localhost:3113"
    }
  },
  "dependencies": {
    "@kalenderjawa/pancawara": "^1.0.0",
    "@kalenderjawa/saptawara": "^1.3.0"
  }
}
